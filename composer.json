{
  "name": "t324inc/d324theme_admin",
  "description": "An admin theme based on Adminimal Theme for D324",
  "type": "drupal-theme",
  "homepage": "https://github.com/t324inc/d324theme_admin",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "T324",
      "homepage": "https://www.t324.com",
      "role": "Maintainer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
      },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "~1.0",
    "oomphinc/composer-installers-extender": "~1.0",
    "cweagans/composer-patches": "~1.0",
    "drupal/drupal-library-installer-plugin": "^0.3",
    "drupal/adminimal_theme": "~1.0"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-7.x": "8.7.x-dev"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "enable-patching": true,
    "composer-exit-on-patch-failure": true
  }
}
